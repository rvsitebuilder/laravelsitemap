{
    "name": "rvsitebuilder/laravelsitemap",
    "type": "rvsitebuilder-app",
    "description": "Generate sitemap.xml and submit to google automatically",
    "license": "see license in LICENSE",
    "keywords": [
        "laravel",
        "laravelsitemap",
        "sitemap"
    ],
    "authors": [
        {
            "name": "RVGlobalSoft",
            "email": "support@rvglobalsoft.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "spatie/laravel-sitemap": "^5.2.11",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Rvsitebuilder\\Laravelsitemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "extra": {
        "component": "package",
        "laravel": {
            "providers": [
                "Rvsitebuilder\\Laravelsitemap\\LaravelsitemapServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": false,
        "classmap-authoritative": false,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
